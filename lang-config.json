{
    "python": {
        "build": "",
        "run": "python3",
        "ext": "py"
    },
    "rust": {
        "build": "rustc -o a.out",
        "run": "./a.out",
        "ext": "rs"
    },
    "c": {
        "build": "gcc -o a.out",
        "run": "./a.out",
        "ext": "c"
    },
    "cpp": {
        "build": "g++ -o a.out",
        "run": "./a.out",
        "ext": "cpp"
    },
    "haskell": {
        "build": "ghc -o a.out",
        "run": "./a.out",
        "ext": "hs"
    },
    "java": {
        "build": "runners/java_build.sh",
        "run": "java -jar a.jar",
        "ext": "java"
    },
    "js": {
        "build": "",
        "run": "node",
        "ext":"js"
    },
    "ruby": {
        "build": "",
        "run": "ruby",
        "ext": "rb"
    },
    "c-sharp": {
        "build": "mcs -out:a.out",
        "run": "./a.out",
        "ext": "cs"
    },
    "php": {
        "build": "",
        "run": "php",
        "ext": "php"
    },
    "ts": {
        "build": "runners/ts_build.sh",
        "run": "runners/ts_run.sh",
        "ext": "ts"
    },
    "fortran": {
        "build": "gfortran -o a.out",
        "run": "./a.out",
        "ext": "f90"
    },
    "cobol": {
        "build": "cobc -x -o a.out",
        "run": "./a.out",
        "ext": "cbl"
    },
    "go": {
        "build": "go build -o a.out",
        "run": "./a.out",
        "ext": "go"
    },
    "v": {
        "build": "v -o a.out",
        "run": "./a.out",
        "ext": "v"
    },
    "kotlin": {
        "build": "kotlinc -include-runtime -d a.jar",
        "run": "java -jar a.jar",
        "ext": "kt"
    },
    "lisp": {
        "build": "",
        "run": "sbcl --script",
        "ext": "lisp"
    }
}